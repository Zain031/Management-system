Kontrak API: 
1.  Users 
	BaseUrl: "/api/auth"
		- Create user:
			method: "POST"
			path:/register
			request_body:{
				"email": "string",
        			"password": "string",
        			"name": "string",
       			 	"role": "SUPER_ADMIN/ADMIN"
      			}
			response: {
				  "id_user": "long",
				  "email": "string",
				  "name": "string",
				  "role": "SUPER_ADMIN/ADMIN"
			}

		- Get all users:
			method: "GET"
			path:/users
			response: [
        				{
          				    "id_user": "long",
					    "email": "string",
					    "name": "string",
					    "role": "SUPER_ADMIN/ADMIN"
        				}
      			]
      		- Get user by id:
      			method: "GET"
      			path:"/user/:id_user"
      			response: {
        				"id_user": "long",
        				"email": "string",
        				"name": "string",
        				"role": "SUPER_ADMIN/ADMIN"
      			}
      		- Update user by id:
      			method: "PUT"
      			path:"/user/update/:id_user"
      			request_body: {
				"email": "string",
				"password": "string",
				"name": "string",
				"role": "SUPER_ADMIN/ADMIN"
			 }
			response: {
				"id_user": "long",
				"email": "string",
				"name": "string",
				"role": "SUPER_ADMIN/ADMIN"
			}
		- Delete user by id:
			method: "DELETE"
			path: "/delete/:id_user"
		- Login User:
			method: "POST",
			path:"/login",
			request_body:{
					"email": "string",
					"password": "string"
			}
			response: {
					"token": "code",
					"user": {
						      "id_user":"long",
						      "email"  :"string",
						      "name"   :"string",
						      "role"   :"SUPER_ADMIN"
					}
			}
2. Foods
	BaseUrl": "/api/foods",
		- Create Food
		      method: "POST",
		      path: "/create",
		      request_body: {
		        "id_user":long,
			"food_name": "string",
			"food_price": "long",
			"total_food": "long"
		      },
		      response: {
			"id_food": "long",
			"name"   : "string",
			"food_name": "string",
			"food_price": "long",
			"total_food": "long",
			"total_food_price": "long",
		      }
	    	- Get all foods
		      method: "GET",
		      path: "/",
		      response: [
			{
			        "id_food": "long",
				"name"   : "string",
				"food_name": "string",
				"food_price": "long",
				"total_food": "long",
				"total_food_price": "long",
			}
		      ]
	    
	    	- Get food by id
		      method: "GET",
		      path: "/:id_food",
		      response: {
			        "id_food": "long",
				"name"   : "string",
				"food_name": "string",
				"food_price": "long",
				"total_food": "long",
				"total_food_price": "long",
		      }
		- Update food by id
		      method: "PUT",
		      path: "/update/:id_food",
		      request_body: {
		        "id_user":long,
			"food_name": "string",
			"food_price": "long",
			"total_food": "long"
		      },
		      response: {
			        "id_food": "long",
				"name"   : "string",
				"food_name": "string",
				"food_price": "long",
				"total_food": "long",
				"total_food_price": "long",
		      }
		 - Delete food by id
		      method: "DELETE",
		      path: "/delete/:id_food"
3. Drinks
	BaseUrl": "/api/drinks",
		- Create Drink
		      method: "POST",
		      path: "/create",
		      request_body: {
		        "id_user":long,
			"drink_name": "string",
			"drink_price": "long",
			"total_drink": "long"
		      },
		      response: {
			"id_drink": "long",
			"name":"string",
			"drink_name": "string",
			"drink_price": "long",
			"total_drink": "long",
			"total_drink_price": "long",
		      }
	    	- Get all drinks
		      method: "GET",
		      path: "/",
		      response: [
			{
				
			        "id_drink": "long",
				"name":"string",
				"drink_name": "string",
				"drink_price": "long",
				"total_drink": "long",
				"total_drink_price": "long",
			}
		      ]
	    
	    	- Get drink by id
		      method: "GET",
		      path: "/:id_drink",
		      response: {
			"id_drink": "long",
			"name":"string",
			"drink_name": "string",
			"drink_price": "long",
			"total_drink": "long",
			"total_drink_price": "long",
		      }
		- Update drink by id
		      method: "PUT",
		      path: "/update/:id_drink",
		      request_body: {
		        "id_user":long,
			"drink_name": "string",
			"drink_price": "long",
			"total_drink": "long"
		      },
		      response: {
			"id_drink": "long",
			"name":"string",
			"drink_name": "string",
			"drink_price": "long",
			"total_drink": "long",
			"total_drink_price": "long",
		      }
		 - Delete drink by id
		      method: "DELETE",
		      path: "/delete/:id_drink"
4. Sales
	BaseUrl: "/api/sales",
	  	- Create Sales
		      method: "POST",
		      path: "/create",
		      request_body: {
		        "id_user": "long",
			"food_name": "string",
			"total_sales_foods": "long",
			"total_sales_foods_price":"long",
			"drink_name": "string",
			"total_sales_drinks":"long",
			"total_sales_drinks_price":"long",
			"date_sales": "date"
		      },
		      response: {
			"id_sales": "long",
			"name": "String",
			"food_name": "string",
			"total_sales_foods": "long",
			"leftover_food_sale": "long",
			"total_sales_foods_price":"long",
			"drink_name": "string",
			"total_sales_drinks":"long",
			"leftover_drink_sale":"long",
			"total_sales_drinks_price":"long",
			"total_sales_price": "long",
			"date_sales": "date",
			"period": "month"
		      }
	    	- Get All Sales
		      method: "GET",
		      path: "/",
		      response: [{
			"id_sales": "long",
			"name": "String",
			"food_name": "string",
			"total_sales_foods": "long",
			"leftover_food_sale": "long",
			"total_sales_foods_price":"long",
			"drink_name": "string",
			"total_sales_drinks":"long",
			"leftover_drink_sale":"long",
			"total_sales_drinks_price":"long",
			"total_sales_price": "long",
			"date_sales": "date",
			"period": "month"
		      }]
	    	- Get Sales By Date
		      method: "GET",
		      path: "/date/:date_sales",
		      response: {
			"id_sales": "long",
			"name": "String",
			"food_name": "string",
			"total_sales_foods": "long",
			"leftover_food_sale": "long",
			"total_sales_foods_price":"long",
			"drink_name": "string",
			"total_sales_drinks":"long",
			"leftover_drink_sale":"long",
			"total_sales_drinks_price":"long",
			"total_sales_price": "long",
			"date_sales": "date",
			"period": "month"
		      }
		 - Get Sales By Month
		      method: "GET",
		      path: "/month/:period",
		      response: [{
			"id_sales": "long",
			"name": "String",
			"food_name": "string",
			"total_sales_foods": "long",
			"leftover_food_sale": "long",
			"total_sales_foods_price":"long",
			"drink_name": "string",
			"total_sales_drinks":"long",
			"leftover_drink_sale":"long",
			"total_sales_drinks_price":"long",
			"total_sales_price": "long",
			"date_sales": "date",
			"period": "month"
		      }]
		
	    	- Update Sales By Id
		      method: "PUT",
		      path: "/update/:id_sales",
		      request_body: {
		        "id_user": "long",
			"food_name": "string",
			"total_sales_foods": "long",
			"total_sales_foods_price":"long",
			"drink_name": "string",
			"total_sales_drinks":"long",
			"total_sales_drinks_price":"long",
			"date_sales": "date"
		      },
		      response: {
			"id_sales": "long",
			"name": "String",
			"food_name": "string",
			"total_sales_foods": "long",
			"leftover_food_sale": "long",
			"total_sales_foods_price":"long",
			"drink_name": "string",
			"total_sales_drinks":"long",
			"leftover_drink_sale":"long",
			"total_sales_drinks_price":"long",
			"total_sales_price": "long",
			"date_sales": "date",
			"period": "month"
		      }
	    
		 - Delete Sales By Id
		      method: "DELETE",
		      path: "/:id_sales",
		      
5. Tools
	BaseUrl: "/api/tools",
	   - Create Tool
	      method: "POST",
	      path: "/create",
	      request_body: {
	        "id_user": "long"
		"tool_name": "string",
		"tool_price_unit": "long",
		"tool_quantity": "long",
		"tool_discount": "long",
		"tool_price_discount": "long",
		"tool_total_price": "long",
		"date_tool_buy": "date",		
	      },
	      response: {
		"id_tool": "long",
		"name":"string",
		"tool_name": "string",
		"tool_price_unit": "long",
		"tool_quantity": "long",
		"tool_discount": "long",
		"tool_price_discount": "long",
		"tool_total_price": "long",
		"date_tool_buy": "date",
		"period":"month"
	      }
	   - Get All Tools
	      method: "GET",
	      path: "/",
	      response: [{
		"id_tool": "long",
		"name":"string",
		"tool_name": "string",
		"tool_price_unit": "long",
		"tool_quantity": "long",
		"tool_discount": "long",
		"tool_price_discount": "long",
		"tool_total_price": "long",
		"date_tool_buy": "date",
		"period":"month"
	      }]
	   - Get Tool By Id
	      method: "GET",
	      path: "/:id_tool",
	      response: {
		"id_tool": "long",
		"name":"string",
		"tool_name": "string",
		"tool_price_unit": "long",
		"tool_quantity": "long",
		"tool_discount": "long",
		"tool_price_discount": "long",
		"tool_total_price": "long",
		"date_tool_buy": "date",
		"period":"month"
	      }
	   - Get Tool By Date Buy
	      method: "GET",
	      path: "/date/:date_tool_buy",
	      response: [{
		"id_tool": "long",
		"name":"string",
		"tool_name": "string",
		"tool_price_unit": "long",
		"tool_quantity": "long",
		"tool_discount": "long",
		"tool_price_discount": "long",
		"tool_total_price": "long",
		"date_tool_buy": "date",
		"period":"month"
	      }]
	   - Get Tool By Month
	      method: "GET",
	      path: "/month/:period",
	      response: [{
		"id_tool": "long",
		"name":"string",
		"tool_name": "string",
		"tool_price_unit": "long",
		"tool_quantity": "long",
		"tool_discount": "long",
		"tool_price_discount": "long",
		"tool_total_price": "long",
		"date_tool_buy": "date",
		"period":"month"
	      }]
	   - Update Tool By Id
	      method: "PUT",
	      path: "/update/:id_tool",
	      request_body: {
	        "id_user": "long"
		"tool_name": "string",
		"tool_price_unit": "long",
		"tool_quantity": "long",
		"tool_discount": "long",
		"tool_price_discount": "long",
		"tool_total_price": "long",
		"date_tool_buy": "date",		
	      },
	      response: {
		"id_tool": "long",
		"name":"string",
		"tool_name": "string",
		"tool_price_unit": "long",
		"tool_quantity": "long",
		"tool_discount": "long",
		"tool_price_discount": "long",
		"tool_total_price": "long",
		"date_tool_buy": "date",
		"period":"month"
	      }
	   - Delete Tool By Id
	      method: "DELETE",
	      path: "/delete/:id_tool"
6. Foodstuffs
	BaseUrl: "/api/foodstuffs",
	   - Create Foodstuff
	      method: "POST",
	      path: "/create",
	      request_body: {
	        "id_user": "long"
		"foodstuff_name": "string",
		"foodstuff_price_unit": "long",
		"foodstuff_quantity": "long",
		"foodstuff_discount": "long",
		"foodstuff_price_discount": "long",
		"foodstuff_total_price": "long",
		"date_foodstuff_buy": "date",		
	      },
	      response: {
		"id_foodstuff": "long",
		"name": "string",
		"foodstuff_name": "string",
		"foodstuff_price_unit": "long",
		"foodstuff_quantity": "long",
		"foodstuff_discount": "long",
		"foodstuff_price_discount": "long",
		"foodstuff_total_price": "long",
		"date_foodstuff_buy": "date",
		"period": "month"
	      }
	   - Get All Foodstuffs
	      method: "GET",
	      path: "/",
	      response: [{
		"id_foodstuff": "long",
		"name": "string",
		"foodstuff_name": "string",
		"foodstuff_price_unit": "long",
		"foodstuff_quantity": "long",
		"foodstuff_discount": "long",
		"foodstuff_price_discount": "long",
		"foodstuff_total_price": "long",
		"date_foodstuff_buy": "date",
		"period": "month"
	      }]
	   - Get Foodstuff By Id
	      method: "GET",
